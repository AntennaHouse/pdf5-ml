Do forever
Say "Hello, World"
End
</fo:block>        
            <fo:block space-before="1.5122em - 3.8673mm">Following is the codeblock template. scale="90" frame="all"</fo:block>
<fo:block font-family="monospace" font-size="0.81em" space-before="4mm" space-after="2mm" white-space="pre" wrap-option="wrap" background-color="rgb(217,217,217)" padding="1mm" start-indent="inherited-property-value(start-indent) + 1mm + (0.5pt)" end-indent="inherited-property-value(end-indent) + 1mm + (0.5pt)" hyphenate="false" axf:word-break="break-all" axf:word-wrap="break-word" border-top="0.5pt solid black" border-bottom="0.5pt solid black" border-left="0.5pt solid black" border-right="0.5pt solid black">&lt;!-- 
 function:	codeblock template
 param:		prmTopicRef
 return:	fo:block
 note:		
 --&gt;
&lt;xsl:template match="*[contains(@class, ' pr-d/codeblock ')]" priority="2"&gt;
    &lt;xsl:param name="prmTopicRef" required="yes"  as="element()?"/&gt;
    &lt;xsl:param name="prmNeedId"   required="yes"  as="xs:boolean"/&gt;
    
    &lt;fo:block&gt;
        &lt;xsl:copy-of select="ahf:getAttributeSet('atsCodeBlock')"/&gt;
        &lt;xsl:copy-of select="ahf:getDisplayAtts(.,'atsCodeBlock')"/&gt;
        &lt;xsl:copy-of select="ahf:getUnivAtts(.,$prmTopicRef,$prmNeedId)"/&gt;
        &lt;xsl:apply-templates&gt;
            &lt;xsl:with-param name="prmTopicRef" select="$prmTopicRef"/&gt;
            &lt;xsl:with-param name="prmNeedId"   select="$prmNeedId"/&gt;
        &lt;/xsl:apply-templates&gt;
    &lt;/fo:block&gt;
&lt;/xsl:template&gt;
</fo:block>

