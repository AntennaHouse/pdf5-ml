			<fo:wrapper id="unique_31_smallpets">
				<fo:block space-before="1.5122em - 3.8673mm">Cats are nice.</fo:block>
				<fo:block space-before="1.5122em - 3.8673mm">Dogs are nice.</fo:block>
				<fo:block space-before="1.5122em - 3.8673mm">Friends of mine really love their hedgehogs.</fo:block>
			</fo:wrapper>
			<fo:wrapper id="unique_31_biggerpets">
